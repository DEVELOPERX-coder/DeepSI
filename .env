# DeepSI - Environment Variables
# Rename this file to .env and update the values

# Server settings
PORT=8080

# Database configuration
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=root
DB_NAME=deepsi_db

# JWT settings
JWT_SECRET=your_secret_key_here
JWT_EXPIRATION=86400

# File upload settings
UPLOAD_DIR=uploads
MAX_FILE_SIZE=5242880 # 5MB